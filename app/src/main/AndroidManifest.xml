<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.manan.dev.shineymca">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:clubName=".SignupAct" />
        <activity
            android:clubName=".BottomNavigator"
            android:theme="@style/UserHomePage">
            <intent-filter>
                <action android:clubName="android.intent.action.MAIN" />

                <category android:clubName="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:clubName=".SingleClubActivity" />
        <activity android:clubName=".RoundActivity"></activity>
    </application>

</manifest>